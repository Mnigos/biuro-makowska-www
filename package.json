{
  "name": "biuro-makowska-www",
  "author": "Mnigos",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.17"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "next lint --ignore-path .gitignore --resolve-plugins-relative-to .",
    "lint:css": "stylelint \"**/*.{css,scss,less}\" --allow-empty-input --ignore-path .gitignore",
    "lint:fix": "yarn lint:js --fix && yarn lint:css --fix",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "4.29.19",
    "@tanstack/react-query-devtools": "4.29.19",
    "axios": "1.4.0",
    "next": "13.4.9",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primereact": "9.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/types": "^17.4.4",
    "@next/eslint-plugin-next": "13.4.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "20.4.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "0.33.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-unicorn": "47.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.2",
    "vitest": "^0.33.0",
    "vitest-mock-extended": "1.1.4",
    "zod": "^3.21.4"
  }
